{
  "name": "kalkulator-biaya-konstruksi",
  "version": "1.0.0",
  "description": "Modular Construction Cost Calculator",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "keywords": ["construction", "calculator", "RAB", "cost-estimation"],
  "author": "Construction Calculator Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
